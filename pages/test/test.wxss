/**index.wxss**/

.container{
  height: 1500rpx;
}
.content {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
  word-break: break-all;
   overflow: hidden;
   -webkit-line-clamp: 1;
 }

.container .search{
  width: 735rpx;
  height: 65rpx;
  padding: 10rpx 10rpx 10rpx 10rpx;
  background: #ffffff;
}
.container .search-left{
  flex: 8;
  background: #ffffff;
  text-align: left;
  
}
.container .search-left input{
  display: inline-block;
  height: 65rpx;
  font-size: 26rpx;
}
.search-placeholder{
  color: #686a6b;
  line-height: 20rpx;
}
.container .search .search-left image{
  display: inline-block;
  width: 35rpx;
  height: 35rpx;
  padding: 15rpx 15rpx 15rpx 20rpx;
}
.container .search .search-right{
  flex: 1;
}
.container .search .search-right image{
  width: 45rpx;
  height: 45rpx;
  padding: 10rpx;
}

.container{
  padding: 0;
  font-size: 14rpx;
  background: #F0F4F3;
  color: #000;
}



/*feed-item part is in app.wxss for multiplexing*/
